{
  "name": "base-vn-node-backend",
  "version": "2.1.0",
  "description": "Node.js proxy backend for the Base.vn public hiring API",
  "main": "src/server.js",
  "type": "commonjs",
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "lint": "eslint src --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "clean": "node ../scripts/clean.cjs",
    "clean:all": "pnpm clean && pnpm install"
  },
  "keywords": [
    "base.vn",
    "proxy",
    "api",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "qs": "^6.13.1"
  },
  "devDependencies": {
    "eslint": "^9.15.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.3.3"
  }
}
