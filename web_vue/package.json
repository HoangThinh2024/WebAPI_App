{
  "name": "web-vue",
  "private": true,
  "version": "2.1.0",
  "type": "module",
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview --host",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write \"src/**/*.{vue,js,json,css}\"",
    "clean": "node ../scripts/clean.cjs",
    "clean:all": "pnpm clean && pnpm install"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "crypto-js": "^4.2.0",
    "dompurify": "^3.3.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/dompurify": "^3.2.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "eslint": "^9.15.0",
    "eslint-plugin-vue": "^9.31.0",
    "prettier": "^3.3.3",
    "vite": "^6.0.5"
  }
}
